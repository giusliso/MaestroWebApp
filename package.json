{
  "name": "maestro-web-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "prettier-check": "prettier-check --parser typescript --single-quote src/**/*.ts && prettier-check --parser typescript --single-quote e2e/**/*.ts && prettier-check --parser typescript --single-quote projects/**/*.ts",
    "prettier-write": "prettier --parser typescript --single-quote --write src/**/*.ts && prettier --parser typescript --write --single-quote e2e/**/*.ts && prettier --parser typescript --single-quote --write projects/**/*.ts",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "inst": "npm install",
    "buildWebUISolution": "npm run inst && npm run test && npm run lint && npm run prettier-write && npm run prettier-check && npm run build-prod"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.10",
    "@angular/cdk": "^7.2.0",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.0",
    "@ng-bootstrap/ng-bootstrap": "^4.0.0",
    "@ngrx/effects": "^6.1.2",
    "@ngrx/router-store": "^6.1.2",
    "@ngrx/store": "^6.1.2",
    "@ngrx/store-devtools": "^6.1.2",
    "core-js": "^2.5.4",
    "ngx-contextmenu": "^5.1.1",
    "primeicons": "^1.0.0",
    "primeng": "^7.0.3",
    "rxjs": "^6.2.2",
    "rxjs-compat": "^6.2.2",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.8.0",
    "@angular-devkit/build-ng-packagr": "~0.8.0",
    "@angular/cli": "^6.2.4",
    "@angular/compiler-cli": "^7.1.4",
    "@angular/language-service": "^6.1.0",
    "@ngrx/schematics": "^6.1.2",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "angular-file": "^0.5.4",
    "codelyzer": "~4.3.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.1.0",
    "prettier": "^1.15.3",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "^3.1.1"
  }
